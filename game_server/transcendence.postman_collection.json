				{
					"name": "getTournamentParticipants",
					"event": [
						{
							"listen": "test",
							"script": {
								"exec": [
									"// JSON'u güvenli şekilde al",
									"const data = pm.response.json().data;",
									"",
									"// tournament_start veya rounds yoksa çık",
									"if (!data.tournament_start || !Array.isArray(data.tournament_start.rounds)) {",
									"    console.log(\"No tournament rounds available yet. Probably not started.\");",
									"    return;",
									"}",
									"",
									"// Tüm round'ları al",
									"const rounds = data.tournament_start.rounds;",
									"",
									"// is_completed === false olan round'u bul",
									"const nextRound = rounds.find(r => r.is_completed === false);",
									"",
									"if (nextRound) {",
									"    const nextRoundNumber = nextRound.round_number;",
									"    pm.globals.set(\"roundNumber\", nextRoundNumber);",
									"    console.log(\"Set roundNumber to:\", nextRoundNumber);",
									"} else {",
									"    console.log(\"No upcoming (incomplete) round found.\");",
									"}",
									""
								],
								"type": "text/javascript",
								"packages": {}
							}
